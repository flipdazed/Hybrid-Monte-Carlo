#pragma once
#ifndef MY_HEADER_H
#define MY_HEADER_H

#include <complex>
#include "myComplex.hpp"
#endif


double ghmc( double p, double ph, double r, double t, double th){

	std::complex<double> numerator;
	std::complex<double> denominator;

	numerator=ghmc(t,ph,th,p,r);

	denominator=1.;

	return (numerator/denominator).real();
}