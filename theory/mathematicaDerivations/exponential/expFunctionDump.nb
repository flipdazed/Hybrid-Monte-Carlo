Cell["\<\
If the function myComplex.hpp doesn\[CloseCurlyQuote]t exist then it can be \
created with this function:\
\>", "Text",
 CellChangeTimes->{{3.679166287061551*^9, 3.6791663066250887`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"makeMyComplex", "[", "outDir_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"myFunc", ",", " ", "saveLoc", ",", " ", "fnName"}], "}"}], ",", 
    "\n", 
    RowBox[{"(*", " ", 
     RowBox[{
     "generate", " ", "the", " ", "save", " ", "name", " ", "and", " ", 
      "location"}], " ", "*)"}], "\n", 
    RowBox[{
     RowBox[{"fnName", " ", "=", " ", "\"\<myComplex.hpp\>\""}], ";", "\n", 
     RowBox[{"saveLoc", "=", 
      RowBox[{
       RowBox[{"FileNameJoin", "[", 
        RowBox[{"{", 
         RowBox[{"outDir", ",", "fnName"}], "}"}], "]"}], "//", 
       "ToString"}]}], ";", "\n", "\n", 
     RowBox[{"(*", 
      RowBox[{"Save", " ", "the", " ", "Function"}], "*)"}], "\n", 
     RowBox[{"myFunc", "=", 
      RowBox[{"StringJoin", "[", "\n", 
       RowBox[{"{", 
